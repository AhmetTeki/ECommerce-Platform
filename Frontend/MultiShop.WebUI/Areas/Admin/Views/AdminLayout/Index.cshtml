@{
  Layout = null;
}

<!DOCTYPE html>
<html lang="en">
  @await Component.InvokeAsync("_HeadPartial")
  <body class="sidebar-mini">
    <div class="wrapper">
      @await Component.InvokeAsync("_MainHeaderPartial")
      @await Component.InvokeAsync("_SidebarPartial")
      @RenderBody()
      @await Component.InvokeAsync("_AdminFooterPartial")
    </div>
    @await Component.InvokeAsync("_AdminScriptPartial")
  </body>
</html>